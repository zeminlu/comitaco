package java.util;
import java.util;

class Vector {	
	Vector() {
		@Requires(true)
		@Ensures(no this.data);
		@Helper
	}	
	
	addElement(Object e) {
		@Ensures(
			this.size() = @old(this.size() + 1)&&
			this.data = @old(this.data ++ (this.size() -> @arg(0)))
		) 
		@Modifies(this.data);		
		@Helper
	}
}
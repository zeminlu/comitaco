package ar.uba.dc.rfm.dynalloy.visualization;

public class DynalloyVisualizerException extends RuntimeException {

	private static final long serialVersionUID = -2948322665229581422L;

	public DynalloyVisualizerException() {
		super();
	}

	public DynalloyVisualizerException(Throwable cause) {
		super(cause);
	}
	
	public DynalloyVisualizerException(String message, Throwable cause) {
		super(message, cause);
	}
}
